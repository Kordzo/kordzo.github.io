.single-img { display:none;}


/*Related Posts*/
.related-posts { background:#f0f0f0; width:630px; padding:20px 10px; overflow:hidden; margin:0 0 26px 0; border-bottom-right-radius:10px; border-bottom-left-radius:10px;}
.related-posts-title { font-size:16px; padding:10px 10px 10px 20px; margin:30px 0 0 0; background:#2A76DB; color:#fff; font-family:bpg_banner;}
.related-box { width:295px; float:left; margin:0 10px;}
.related-box img { width:295px; height:135px;}
.related-box img:hover {opacity: 0.8; filter: alpha(opacity=80); /* For IE8 and earlier */}
.related-posts p { color:#272727; font-size:14px; margin:10px 0 0 0; font-family:bpg_banner; line-height:1.4;}
.related-posts p a { color:#272727;}
.related-posts p a:hover { color:#2A76DB;}



/*WP Editor Style*/
.content-text { color:#272727; font-size:14px; margin:20px 0; line-height:1.8;}
.content-text a { color:#41B7D8; font-size:14px; text-decoration:none; line-height:1.8; border-bottom:solid 1px #41B7D8;}
.content-text a:hover { text-decoration:none; border-bottom:solid 0px #41B7D8;}
.content-text p { margin:20px 0 0 0;}
blockquote { color:#272727; font-size:14px; font-style:italic; background:#f7f7f7; padding:10px 30px 30px 30px; margin:30px 0;}

.content-text ul li { font-size:14px; color:#272727; background:url(imgs/li-dot.png) no-repeat left top 8px; padding:0 0 0 12px; margin:0 0 10px 20px; color:#808080;}
.content-text ul li ul li { margin:10px 0 0 30px;}
.content-text ul li ol li { margin:10px 0 0 30px;}

.content-text ol li { font-size:14px; color:#272727; margin:0 0 20px 0px; color:#272727;}
.content-text ol li ul li { margin:10px 0 0 30px;}
.content-text ol li ol li { margin:10px 0 0 30px;}

.content-text h1 { font-family:bpg_banner; font-weight:normal; font-size:24px; color:#272727;  margin:40px 0 20px 0;}
.content-text h1 a { font-weight:normal; font-size:24px; color:#41B7D8; text-decoration:none;}
.content-text h1 a:hover { text-decoration:none;}

.content-text h2 { font-family:bpg_banner; font-weight:normal; font-size:22px; color:#272727;  margin:30px 0 20px 0;}
.content-text h2 a { font-weight:normal; font-size:22px; color:#41B7D8; text-decoration:none;}
.content-text h2 a:hover { text-decoration:none;}

.content-text h3 { font-family:bpg_banner; font-weight:normal; font-size:20px; color:#272727;  margin:20px 0 20px 0;}
.content-text h3 a { font-weight:normal; font-size:20px; color:#41B7D8; text-decoration:none;}
.content-text h3 a:hover { text-decoration:none;}

.content-text h4 { font-family:bpg_banner; font-weight:normal; font-size:18px; color:#272727;  margin:20px 0 20px 0;}
.content-text h4 a { font-weight:normal; font-size:18px; color:#41B7D8; text-decoration:none;}
.content-text h4 a:hover { text-decoration:none;}

.content-text h5 { font-family:bpg_banner; font-weight:normal; font-size:16px; color:#272727;  margin:20px 0 20px 0;}
.content-text h5 a { font-weight:normal; font-size:16px; color:#41B7D8; text-decoration:none;}
.content-text h5 a:hover { text-decoration:none;}

.content-text h6 { font-family:bpg_banner; font-weight:normal; font-size:14px; color:#272727;  margin:20px 0 20px 0;}
.content-text h6 a { font-weight:normal; font-size:14px; color:#41B7D8; text-decoration:none;}
.content-text h6 a:hover { text-decoration:none;}

.content-text img { max-width:650px; width:100%; height:auto; margin:0 0 10px 0;}

.wp-caption-text{display: none;}



/*sidebar*/
.sidebar { width:270px; padding:0 0 0 28px; float:left;}
/*Categories*/
.category { margin:20px 0 0 0;}
.category ul li { padding:0 0 20px 0;}
.category ul li:hover {}
.category ul li a { background:url(imgs/cat-dot.png) no-repeat left top 0px; color:#272727; font-size:14px; text-shadow: 1px 1px #fff; padding:0 0 0 26px; font-family:bpg_banner;}
.category ul li a:hover { color:#2A76DB; background:url(imgs/cat-dot-hover.png) no-repeat left top 0px;}

.sidebar-titles { font-family:bpg_banner; font-size:18px; color:#272727; border-bottom:solid 1px #2A76DB; margin:0px 0 0 0; padding:0 0 30px 0;}

.tags a:hover { text-decoration:underline;}
.vidget_title { font-family:bpg_banner; font-size:18px; color:#272727; border-bottom:solid 1px #2A76DB; margin:30px 0 20px 0; padding:0 0 30px 0;}


.tagcloud a{ color:#272727;}


/*AUTHOR*/
.media, .bd { overflow: hidden;}
.bd h4 { font-weight:normal; font-family:bpg_banner;}
.bd p { font-size:13px; color:#808080; margin:6px 0 0 0;}
.media img, .media .img { float: left; margin-right: 10px; }
.media img { display: block; border-radius:50px; border:solid 1px #eee;}
.media .imgExt { float: right; margin-left: 10px; }
.profile { margin-top: 10px; padding: 20px 10px; background:#FCFCFC; border:solid 1px #F5F5F5;}
.profile-links { padding: 5px 0; }

.social-links li { padding: 0px 20px 0px 0px; display: inline; list-style: none; text-indent: -9999px; float: left; }
.social-links li a { background-position: 0 0; background-repeat: no-repeat; width: 16px; height: 16px; display: block; }
.twitter-link { background: url( imgs/twitter.png ) no-repeat; }
.facebook-link { background: url( imgs/facebook.png ) no-repeat; }
.linkedin-link { background: url( imgs/linkedin.png ) no-repeat; }
.google-link { background: url( imgs/googleplus.png ) no-repeat; }



/*--------PAGINATION-------------------------*/
div.pagination {
		padding: 3px;
		margin: 3px;
		text-align:center;
}

div.pagination a, div.pagination span.pages, div.pagination span.dots, div.pagination span.current {
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  -o-border-radius: 3px;
  -ms-border-radius: 3px;
  -khtml-border-radius: 3px;
  border-radius: 3px;
  margin: 1px 2px;
  padding: 8px 14px;
  display: inline-block;
  text-decoration: none !important;
  color: #2A76DB !important;
  font-size: 14px !important;
  font-family: "Helvetica Neueu", Helvetica, Arial, sans-serif;
  background-color: #eee;
}

div.pagination a:hover, div.pagination a:focus {
  color: #fff !important;
  background-color: #2A76DB;
}

div.pagination span.current {
  color: #fff !important;
  background-color: #2A76DB;
}

.mob-pagination { display:none;}


/*SOCIAL MEDIA*/
.social-bottom { background:#fff; overflow:hidden; margin:50px 0 0 0;}
.social-bottom ul li { display:inline-block;}
.social-bottom ul li.comment-title { background:#F1F1F1; font-size:14px; color:#272727; padding:10px 15px 26px 15px; border-radius:3px; float:left; margin:6px 0 0 0; font-weight:normal; font-family:bpg_banner;}
.social-bottom ul li.social-botton-float { float:right;}

.social { height:50px !important; width:650px;}
.social-single { height:50px !important; width:650px; margin:20px 0 0 0;}

/* General Social Button Styling */
.social-icons {
	height: 35px;
}

.social-icons li {
	list-style-type: none !important;
	float: left;
	margin-top: 14px;
	margin-right: 10px;
	margin-bottom: 10px;
	margin-left: 0 !important;
	font-size: 12px !important;
	font-weight: bold !important;
}

.social-icons li:last-child {
	margin-right: 0 !important;
}

/* Facebook Button Styling */
li.social-icon.facebook a {
	border-top: 1px solid #3b5998;
	border-left: 1px solid #3b5998;
	border-bottom: 1px solid #3b5998;
	padding: 8px 5px 8px 25px;
	-webkit-border-top-left-radius: 3px;
	border-top-left-radius: 3px;
	-webkit-border-bottom-left-radius: 3px;
	border-bottom-left-radius: 3px;
	color: #fff;
	background:url(imgs/facebook-hover.png) no-repeat left 3px top 6px #3b5998;
	
	font-family: 'Helvetica Neue', Helvetica, Arial, 'lucida grande', tahoma, verdana, arial, sans-serif;
	text-decoration: none;
}

li.social-icon.facebook a:hover {
	color: #3b5998;
	background:url(imgs/facebook.png) no-repeat left 3px top 6px #ffffff;
}

li.social-icon.facebook span.share-count {
	border: 1px solid #3b5998;
	padding: 8px 10px;
	-webkit-border-top-right-radius: 3px;
	border-top-right-radius: 3px;
	-webkit-border-bottom-right-radius: 3px;
	border-bottom-right-radius: 3px;
	color: #3b5998;
	font-family: 'Helvetica Neue', Helvetica, Arial, 'lucida grande', tahoma, verdana, arial, sans-serif;
}

